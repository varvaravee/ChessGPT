#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <libgen.h>
#include <string.h>






enum PieceType{
    none=0,
    Pawn,
    Rook,
    Bishop,
    Knight,
    Queen,
    King,
}

enum Player{
    Black,
    White,
}

enum pieceFile{
A,
B,
C,
D,
E,
F,
G,
H,
}

struct ChessPiece{
    enum PieceType type;
    enum Player player;
    enum pieceFile piecefile;
    int rank;
}


struct ChessPiece board[8][8]

