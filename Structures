#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <libgen.h>
#include <string.h>






enum PieceType{
    none=0,
    Pawn,
    Rook,
    Bishop,
    Knight,
    Queen,
    King,
}

enum Player{
    Black,
    White,
}

struct ChessPiece{
    enum PieceType type;
    enum Player player;
}

struct ChessPiece board[8][8]

